public class startOfGame{
    String name;
    String characterClass;
    public nullBackground(){
        name = “”;
        characterClass = “”;
    }
    public void gameBackground(){
        System.out.println("Welcome to the world of Javah, your typical fantasy game.");
        System.out.println("In this world you must battle monsters and gain strength in order to defeat the five titans.");
        System.out.println("What is your name, brave adventurer?");       
    }
}
